{
  "name": "@indielayer/ui",
  "version": "1.0.3",
  "description": "Indielayer UI Components with Tailwind CSS build for Vue 3",
  "author": {
    "name": "JoÃ£o Teixeira",
    "url": "https://indielayer.com/"
  },
  "keywords": [
    "vue",
    "vue 3",
    "tailwindcss",
    "components",
    "ui framework",
    "component framework",
    "ui library",
    "component library",
    "indielayer"
  ],
  "files": [
    "lib",
    "src",
    "*.d.ts",
    "README.md"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.cjs.js"
    },
    "./nuxt": "./lib/nuxt.js",
    "./style": "./lib/style.css",
    "./styles": "./lib/style.css",
    "./nuxt.plugin": "./lib/nuxt.plugin.js",
    "./nuxt.plugin.js": "./lib/nuxt.plugin.js",
    "./tailwind.preset": "./lib/tailwind.preset.js",
    "./tailwind.preset.js": "./lib/tailwind.preset.js"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite build --watch",
    "dev2": "pnpm build-only && chokidar \"./src/**\" -c \"pnpm build-only\"",
    "build": "pnpm generate && vite build",
    "build-prod": "pnpm build && pnpm gen-types",
    "build-only": "vite build",
    "generate": "pnpm gen-version",
    "gen-types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.vitest.json --composite false",
    "gen-version": "node .scripts/gen-version.js",
    "test": "pnpm test:unit",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.1",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.30",
    "@types/ramda": "^0.28.15",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^8.4.2",
    "autoprefixer": "^10.4.0",
    "c8": "^7.12.0",
    "chokidar-cli": "^3.0.0",
    "clean-css": "^5.3.0",
    "cypress": "^9.6.0",
    "eslint": "^8.16.0",
    "eslint-plugin-cypress": "^2.12.1",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.4",
    "ramda": "^0.28.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.0",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.15"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/indielayer/ui/issues"
  },
  "homepage": "https://github.com/indielayer/ui#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indielayer/ui.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
